.systems-availability-table {
  border: 0;
  thead {
    background-color: #fff;
    color: $body-font-color;
    tr {
      th {
        border-top: 0;
        font-size: 16px;
      }
    }
  }
  tbody {
    tr {
      td {
        border-top: 0;
      }
    }
  }

}